<div class="row">
<!-- left column -->
<div class="col-md-6">
    <!-- general form elements -->
    <div class="box box-primary">
        <div class="box-header">
            <h3 class="box-title">树</h3>
        </div><!-- /.box-header -->
        <!-- form start -->
        <form role="form">
            <div class="box-body">
                {{bu-tree options=view.treeOptions zNodes=view.zNodes}}
            </div><!-- /.box-body -->
        </form>
    </div><!-- /.box -->
</div><!--/.col (left) -->
<!-- right column -->
<div class="col-md-6">
    <!-- general form elements disabled -->
    <div class="box box-warning">
        <div class="box-header">
            <h3 class="box-title">日志</h3>
        </div><!-- /.box-header -->
        <div class="box-body">
            <div id="logHolder">
            </div>
        </div><!-- /.box-body -->
    </div><!-- /.box -->
</div><!--/.col (right) -->
</div>   <!-- /.row -->